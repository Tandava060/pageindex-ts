{
  "name": "pageindex-ts",
  "version": "1.0.1",
  "description": "LLM-agnostic document indexing for js/ts - bring your own LLM and text",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pdf",
    "markdown",
    "tree",
    "index",
    "document",
    "rag",
    "llm",
    "ai"
  ],
  "author": "Tandava Appadoo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tandava060/pageindex-ts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}